{
    "name": "skeleton/skeleton-php-application",
    "description": "Este é uma estrutura de projeto em PHP puro moderno usando os mais variados recursos disponível na comunidade.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Skeleton\\SkeletonPhpApplication\\": "App/"
        }
    },
    "authors": [
        {
            "name": "Caique Bispo",
            "email": "caiquebispodanet86@gmail.com"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^12.2",
        "illuminate/routing": "^12.3",
        "illuminate/http": "^12.3",
        "illuminate/container": "^12.3",
        "illuminate/events": "^12.3"
    },
    "scripts": {
        "migration-create": "Skeleton\\SkeletonPhpApplication\\Core\\Console\\Migration::create",
        "controller-create": "Skeleton\\SkeletonPhpApplication\\Core\\Console\\Controller::create",
        "model-create": "Skeleton\\SkeletonPhpApplication\\Core\\Console\\Model::create"
    }
}
